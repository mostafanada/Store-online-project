{
    "spec_dir": "build",
    "spec_files": [
      "**/*_spec.js",
      "build/tests/*.js"
    ],
    "helpers": [
      "tests/helpers/**/*.js"
    ],
    "reporters": [
      {
        "name": "jasmine-spec-reporter#SpecReporter",
        "options": {
          "displayStacktrace": "all"
        }
      }
    ],
    "stopSpecOnExpectationFailure": false,
    "random": false
  }